{
    "name": "Brother QL App",
    "version": "1.1.0",
    "slug": "brother_ql_app",
    "description": "A Home Assistant Add-on for the Brother QL label printer.",
    "arch": [
      "armhf",
      "armv7",
      "aarch64",
      "amd64",
      "i386"
    ],
    "startup": "application",
    "boot": "auto",
    "options": {},
    "schema": {},
    "image": "dodoooh/brother_ql_app",
    "host_network": true,
    "ports": {
      "5000/tcp": 5000
    },
    "ingress": false,
    "panel_icon": "mdi:printer",
    "map": [
      "config",
      "ssl",
      "share",
      "media"
    ],
    "webui": "http://[HOST]:[PORT:5000]",
    "environment": [
      {
        "name": "EXAMPLE_ENV",
        "value": "example_value"
      }
    ],
    "privileged": [
      "NET_ADMIN",
      "SYS_ADMIN"
    ]
  }